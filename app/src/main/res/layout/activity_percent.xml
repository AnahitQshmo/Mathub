            android:layout_marginVertical="2dp"
            android:textColor="@color/white"
            android:textSize="28dp"
            android:typeface="serif"
            tools:ignore="HardcodedText,SpUsage,TextSizeCheck,VisualLintLongText" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="@string/range_text2"
            android:layout_marginVertical="6dp"
            android:textColor="@color/white"
            android:textSize="38dp"
            android:typeface="serif"
            tools:ignore="HardcodedText,SpUsage,TextSizeCheck,VisualLintLongText" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="@string/str32"
            android:textColor="@color/white"
            android:textSize="28dp"
            android:typeface="serif"
            tools:ignore="HardcodedText,SpUsage,TextSizeCheck,VisualLintLongText" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginVertical="5dp"
            android:text="@string/str33"
            android:textColor="@color/white"
            android:textSize="28dp"
            android:typeface="serif"
            tools:ignore="HardcodedText,SpUsage,TextSizeCheck,VisualLintLongText" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginVertical="5dp"
            android:text="@string/str34"
            android:textColor="@color/white"
            android:textSize="28dp"
            android:typeface="serif"
            tools:ignore="HardcodedText,SpUsage,TextSizeCheck,VisualLintLongText" />

        <Button
            android:id="@+id/task_button5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="75dp"
            android:layout_marginEnd="75dp"
            android:minWidth="140dp"
            android:text="@string/task_button"
            android:typeface="serif"
            android:textSize="22dp"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="HardcodedText,SpUsage,TextSizeCheck,VisualLintButtonSize" />


    </LinearLayout>
</ScrollView>